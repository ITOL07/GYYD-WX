/* container1 */
.container1{
	background-color: #FAFAFA;
}

/* top */
.top{
	width: 100%;
	height: 500rpx;
	text-align: center;
	background-color: #FFFFFF;
	margin-bottom: 20rpx;
	justify-content: space-around;
}
.time{
	font-size: 30px;
	font-family: PingFangSC-Medium;
	font-weight: 500;
	color: #666666;
	margin-top: 40rpx;
	margin-bottom: -30rpx;
}
.text{
	font-size: 11px;
	font-family: PingFangSC-Regular;
	font-weight: 400;
	color: #666666;
}
.inside{
	width: auto;
	margin-left: 25rpx;
	white-space: nowrap;
}
.inside2{
	width: auto;
	white-space: nowrap;
}
.space{
	width: 220rpx;
	height: 220rpx;
	background-color: #E5E5E5;
	border-radius: 5%;
	margin-right: 25rpx;
	display: inline-block;
}

/* bottom */
.bottom{
  width:100%;
  height:auto;
  background:#FFFFFF;
  margin-top:20rpx;
	padding-bottom: 40rpx;
  display:flex;
  flex-direction:column;
}
.bottom .body{
  width:90%;
  height:100rpx;
  font-size:35rpx;
  color:#333333;
  margin:0 auto;
}
.bottom .list{
  width:90%;
  height:100rpx;
	border-bottom: 1rpx solid #EEEEEE;
  display:flex;
  flex-direction:row;
	align-items: center;
  margin:0 auto;
}
.bottom .list image{
  display:block;
  width:40rpx;
  height:40rpx;
}
.bottom .list text:nth-child(2){
  width:63%;
  height:50rpx;
  font-size:28rpx;
  color:#333333;
  text-align:left;
	line-height: 50rpx;
  display:block;
  margin-left:3%;
}
.bottom .list text:last-child{
  color:#333333;
  width:63%;
  height:50rpx;
  font-size:28rpx;
	line-height: 50rpx;
  /* color:#333333; */
  text-align:right;
  display:block;
}
.tips{
	width: 100%;
	background-color: #FFFFFF;
	font-size: 28rpx;
	color: #666666;
	line-height: 200rpx;
	text-align: center;
}