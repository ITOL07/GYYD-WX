.containe{
	width: 100%;
	height: 1207rpx;
	background-color: #EEEEEE;
}
.top{
	width: 94%;
	height: 100rpx;
	background-color: #FFFFFF;
	padding: 0 3%;
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: space-between;
}
.top text{
	font-size: 30rpx;
	color: #333333;
}
.top view{
	display: flex;
	flex-direction: row;
	align-items: center;
}
.top view image:nth-child(1){
	width: 40rpx;
	height: 40rpx;
	border-radius: 50%;
	margin-right: 20rpx;
}
.top image:nth-child(2){
	width: 20rpx;
	height: 40rpx;
}
.bottom{
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	font-size: 28rpx;
	color: #666666;
	text-align: center;
	text-decoration: underline;
}
.num{
	height: 100rpx;
	font-size: 25rpx;
	color: #666666;
	line-height: 100rpx;
	padding: 0;
}
.call{
	height: 100rpx;
	line-height: 100rpx;
	padding: 0;
}