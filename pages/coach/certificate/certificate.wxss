@keyframes fadeIn {
    0% { opacity: 0; }
    100% { opacity: 1; }
}
.container{
	 background-color: #FFFFFF;
}
.header{
  width: 90%;
	margin-top: 2%; 
	display: flex;
	flex-direction: row;
	align-content: center;
	justify-content: space-between;
	flex-wrap: wrap;
}
.imgview{
	display: inline-block;
	width: 329rpx;
  height: 233rpx;
  border-radius: 5px;
	margin: 2% 0;
}
.imgview image{
  width: 100%;
  height: 100%;
}
.fade_in {
    animation: fadeIn 1s both;
}
