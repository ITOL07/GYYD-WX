/* pages/privatedetail/privatedetail.wxss */
.container{
  background:#FFFFFF;
}
.private_header{
  width: 95%;
  height: 350rpx;
  display:flex;
  flex-direction:row;
  margin: 20rpx 2.5%;
}
.header_left{
  width:20%;
  height:100%;
}
.header_left image{
  width: 120rpx; 
  height: 120rpx;
	border-radius: 50%;
	margin-top: 15rpx; 
}
.header_right{
  width:80%;
  height:100%;
}
.header_right_top{
  width:100%;
  height:50%;
	border-bottom: 2rpx solid #EEEEEE;
}
.name{
  width:100%;
  height:33%;
}
.name text:nth-child(1){
  color:#333333;
  font-size: 32rpx;
	font-weight: 600rpx;
}
.name text:nth-child(2){
  color:#666666;
  font-size: 22rpx;
  margin-left:5%;
}
.rate{
  width: 30%;
  height:33%;
  display:flex;
  flex-direction:row;
	align-items: center;
	justify-content: space-around;
}
.rate image{
  width: 19rpx;
  height: 19rpx;
}
.rate text{
  font-size: 22rpx;
	color: #333333;
}
.phone{
  width: 20%;
  height:33%;
  display:flex;
  flex-direction:row;
	align-items: center;
	justify-content: space-around;
}
.phone image:nth-child(1){
  width: 22rpx;
  height: 18rpx;
}
.phone image:nth-child(2){
  width: 18rpx;
  height: 22rpx;
}
.header_right_bottom{
  width: 90%;
  height:50%;
  display:flex;
  flex-direction:row;
	justify-content: space-between;
}
.header_right_bottom view{
	height: 150rpx;
	margin-top: 30rpx;
  display: flex;
	flex-direction: column;
}
.header_right_bottom image{
  width: 60rpx;
  height: 54rpx;
	margin-bottom: 24rpx;
}
.header_right_bottom text{
  color:#999999;
  font-size: 24rpx;
	margin-left: -12rpx;
}
.private_middle{
  width: 94%;
  height:auto;
  display:flex;
  flex-direction:column;
	margin: 0 3%;
}
.private_middle .title{
  color:#333333;
  font-size: 30rpx;
}
.private_middle .introduce{
  color:#666666;
  font-size: 24rpx;
  margin-top:20rpx;
  margin-bottom: 50rpx;
}
.place{
  width:100%;
  display:flex;
  flex-direction:column;
}
.place_header{
	width: 100%;
  height: 30%;
	margin-left: 3%;
}
.place_header text{
  font-size: 30rpx;
  color:#333333;
	margin-bottom: 20rpx;
}
.place_bottom{
  width:93%;
  height:90%;
  display:flex;
  flex-direction:row;
  margin:10rpx auto;
}
.place_all{
  width:32%;
  height:100%;
}
.place_all image{
  display:block;
  width:210rpx;
  height:210rpx;
}
.place_all text{
  font-size:30rpx;
  color:#666666;
}

.course{

}
.private_head{
	margin-left: 3%;
}
.listall{
  width: 100%;
  height: 900rpx;
	margin-top: 10rpx;
  display:flex;
  flex-direction:row;
	flex-wrap: wrap;
  justify-content:space-between;
}
.private_head text{
  font-size: 30rpx;
  color:#333333;
}
.private_list{
  width: 44%;
	height: 430rpx;
	margin: 30rpx 3%;
}
.list_item{
	width: 100%;
	height: 100%;
  display:flex;
  flex-direction:column;
	justify-content: space-around;
}
.list_item image{
  width: 320rpx;
  height: 260rpx;
}
.private_list text:nth-child(2){
  width:100%;
  font-size: 30rpx;
  color:#666666;
}
.private_list text:nth-child(3){
  width:100%;
  font-size: 22rpx;
  color:#999999;
	overflow: hidden;
	-webkit-line-clamp: 1;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.buy{
  
}
.buy text:nth-child(1), .buy text:nth-child(3){
	font-size: 22rpx;
  color:#F85F58;
}
.buy text:nth-child(2){
	font-size: 32rpx;
  color:#F85F58;
}
.comment{
  width:100%;
  height:auto;
  display:flex;
  flex-direction:column;
  background:#FFFFFF;
  margin-top:10rpx;
}
.comment_header{
  width:90%;
  height:70rpx;
  margin:10rpx auto;
}
.comment_header text{
  font-size:35rpx;
  color:#333333;
}
.comment_title{
  width:90%;
  height:100rpx;
  display:flex;
  flex-direction:row;
  margin:10rpx auto;
}
.comment_title .comment_title_head{
  width:20%;
  height:100%;
}
.comment_title .comment_title_head image{
  display:block;
  width:100rpx;
  height:100rpx;
}
.comment_title .comment_title_name{
  width:50%;
  height:100%;
  display:flex;
  flex-direction:column;
  padding-top:1%;
}
.comment_title .comment_title_name .text1{
  font-size:35rpx;
  color:#333333;
  margin-bottom:1%;
}
.comment_title .comment_title_name .text2{
  font-size:28rpx;
  color:#999999;
}
.comment_title .xing{
  width:30%;
  height:100%;
  display:flex;
  flex-direction:row;
  padding-top:5%;
  
}
.comment_title .xing image{
  display:block;
  width:20rpx;
  height:20rpx;
  margin-right:10rpx;
}
.content{
  width:90%;
  height:auto;
  margin-left:3%;
}
.content text{
  font-size:26rpx;
  color:#666666;
}
.comment_img{
  width:90%;
  height:200rpx;
  display:flex;
  flex-direction:row;
  margin:20rpx auto;
}
.comment_img image{
  display:block;
  width:200rpx;
  height:200rpx;
  margin-right:20rpx;
}
.comment_footer{
  width:90%;
  height:100rpx;
  margin:0 auto;
}
.comment_footer text{
  color:#999999;
  font-size:28rpx;
}
.scroll_body2{
  width: 94%;
  height: 300rpx;
	margin-top: 20rpx;
	margin-bottom: 30rpx;
  display:flex;
  flex-direction:row;
  white-space: nowrap;
}
.nav_bottom{
  width: auto;
  height: 300rpx;
  display:flex;
  flex-direction:row;
  box-sizing:border-box;
}
.nav_all{
  width: 210rpx;
  height: 100%;
	background-color: #FFFFFF;
  margin-right: 10rpx;
  display:inline-block;
  flex-direction:column;
}
.nav_all view{
	width: 100%;
	height: 300rpx;
	border: 2rpx solid #EEEEEE;
	box-shadow:0px 3px 7px 0px rgba(0, 0, 0, 0.04);
	border-radius:4px;
	display: flex;
	flex-direction: column;
	justify-content: space-around;
}
.nav_all image{
  width: 210rpx;
  height: 208rpx;
}
.nav_all text{
  width: 96%;
  font-size: 22rpx;
	color: #666666;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
	margin: 0 3%;
}