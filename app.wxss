/**app.wxss**/
@import 'weui.wxss';
.container{
		width: 100%;
  margin: 0;
	border: 0;
	padding: 0;
  background:#F1F1F1;
  box-sizing:border-box;
  font-family:"微软雅黑";
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:space-between;
}
.scroll_body{
  width:100%;
  display:flex;
  flex-direction:column;
  flex:0.5;
  height:1300rpx;
}
.displayrow{
	display: flex;
	flex-direction: row;
}
.displaycolumn{
	display: flex;
	flex-direction: column;
}